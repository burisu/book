-# Auto-generated by rake :scaff
=toolbar do |t|
  -if @member.person
    -t.show @member.person
  -t.edit @member
  -t.destroy @member, :method => :delete, :confirm => 'Êtes-vous sûr(e) de vouloir supprimer cet enregistrement'
=fieldset :general do
  =attributes_list @member do |l|
    -l.attribute :last_name
    -l.attribute :first_name
    -l.attribute :photo
    -l.attribute :nature
    -l.attribute :other_nature
    -l.attribute :sex
    -l.attribute :phone
    -l.attribute :fax
    -l.attribute :mobile
    -l.attribute :comment
    -l.attribute :person, :label => :label, :url => true
    -l.attribute :email
=fieldset :meta do
  =attributes_list @member do |l|
    -l.attribute :id
    -l.attribute :created_at
    -l.attribute :updated_at
    -l.attribute :lock_version
